{
  "version": 2,
  "name": "vechain-document-notarization",
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/lookup",
      "dest": "/lookup.html"
    },
    {
      "src": "/lookup-data/(.*)",
      "dest": "/lookup-data/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "functions": {},
  "env": {
    "VECHAIN_NODE_URL": "https://testnet.veblocks.net",
    "NODE_ENV": "production"
  }
}